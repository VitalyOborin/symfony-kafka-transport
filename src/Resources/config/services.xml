<?xml version="1.0" encoding="UTF-8" ?>
<container  xmlns="http://symfony.com/schema/dic/services"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://symfony.com/schema/dic/services
            https://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>
        <service id="VO\KafkaTransport\Messenger\KafkaJsonSerializer" class="VO\KafkaTransport\Messenger\KafkaJsonSerializer" />
        <service id="VO\KafkaTransport\Messenger\KafkaTransportFactory" class="VO\KafkaTransport\Messenger\KafkaTransportFactory" public="false">
            <tag name="messenger.transport_factory" />
            <argument type="service" id="logger" on-invalid="null" />
            <argument type="service" id="VO\KafkaTransport\Messenger\KafkaJsonSerializer" on-invalid="null" />
        </service>
    </services>
</container>